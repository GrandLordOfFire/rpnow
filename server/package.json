{
  "name": "rpnow",
  "private": true,
  "scripts": {
    "start": "node src/index.js",
    "start-dev": "nodemon src/index.js",
    "build-nexe": "nexe src/index.js -r node_modules/sqlite3 -r node_modules/knex -r ../client/dist/rpnow",
    "build-pkg": "pkg src/index.js -t node8-linux-x64",
    "lint": "eslint src/**/*.js src/*.js",
    "test": "jasmine --config=./jasmine.json"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "cuid": "^2.1.4",
    "express": "^4.16.4",
    "got": "^9.3.2",
    "knex": "^0.15.2",
    "nanoid": "^1.0.2",
    "normalize-json": "^0.1.3",
    "sqlite3": "^4.0.3",
    "winston": "^2.3.1",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "jasmine": "^2.5.2",
    "nexe": "^3.0.0-beta.9",
    "nodemon": "^1.11.0",
    "pkg": "^4.3.4"
  },
  "bin": "src/index.js",
  "pkg": {
    "assets": "../client/dist/rpnow/**"
  }
}
