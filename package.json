{
  "name": "rpnow",
  "version": "0.2.0",
  "description": "The Dead-Simple Roleplay Chatroom Service",
  "main": "src/index.js",
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "mongojs": "^2.4.0",
    "morgan": "^1.7.0",
    "socket.io": "^1.7.2"
  },
  "devDependencies": {
    "jasmine": "^2.5.2",
    "request": "^2.75.0",
    "socket.io-client": "^1.7.2"
  },
  "scripts": {
    "start": "node src/index.js",
    "pretest": "mkdir -p /tmp/rptestdb && mongod --dbpath /tmp/rptestdb --fork --logpath /dev/null --smallfiles",
    "test": "jasmine JASMINE_CONFIG_PATH=tests/support/jasmine.json",
    "posttest": "mongo admin --eval 'db.shutdownServer()' > /dev/null && rm -rf /tmp/rptestdb"
  },
  "keywords": [
    "roleplay"
  ],
  "author": "Nigel Nelson",
  "license": "ISC"
}
